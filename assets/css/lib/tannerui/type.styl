h1, h2, h3, h4, h5, h6,
.h1, .h2, .h3, .h4, .h5, .h6 {
  font-family: $font-stack;
  font-weight: normal;
  color: $default-headings-color;
  small,
  .small {
    font-weight: normal;
    line-height: 1;
    color: $default-headings-color;
  }
}

h1, .h1,
h2, .h2,
h3, .h3 {
  margin-top: $line-height-computed;
  margin-bottom: $half-line-height-computed;

  small,
  .small {
    font-size: 65%;
  }
}
h4, .h4,
h5, .h5{
	font-weight: bold;
}
h4, .h4,
h5, .h5,
h6, .h6 {
  margin-top: $half-line-height-computed;
  margin-bottom: $half-line-height-computed;

  small,
  .small {
    font-size: 75%;
  }
}

h1, .h1 { font-size: $font-size-h1; }
h2, .h2 { font-size: $font-size-h2; }
h3, .h3 { font-size: $font-size-h3; }
h4, .h4 { font-size: $font-size-h4; }
h5, .h5 { font-size: $font-size-h5; }
h6, .h6 { font-size: $font-size-h6; }
